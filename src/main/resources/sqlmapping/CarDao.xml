<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Car">
    <select id="findCarIdByLocationId" parameterType="java.lang.String" resultType="java.lang.Integer">
        <![CDATA[
        SELECT
            h.carId
        FROM
            h_device as h
        WHERE
            h.deviceId = #{value}
        ]]>
    </select>
</mapper>
