<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="RouteHistory">
    <select id="insertRouteHistory" parameterType="java.util.Map">
        <![CDATA[
        INSERT INTO
        h_route_history
        (`carId`,`longitude`,`latitude`,`time`)
        VALUES (#{carId},#{longitude},#{latitude},#{time})
        ]]>
    </select>
</mapper>
